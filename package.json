{
  "name": "clf-date",
  "version": "0.1.0",
  "description": "Return the current date in CLF format",
  "main": "./out/main.js",
  "bin": {
    "clf-date": "./bin/clf-date.js"
  },
  "files": [
    "bin/clf-date.js",
    "out/main.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "babel src --presets babel-preset-latest --no-comments --compact --out-dir out",
    "cover": "istanbul cover _mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run build",
    "test": "mocha test/index.js --compilers js:babel-core/register",
    "lint": "eslint ."
  },
  "keywords": [
    "clf-date",
    "clf",
    "common-log-format",
    "common",
    "log",
    "format"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lludol/clf-date.git"
  },
  "author": "lludol",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-latest": "^6.24.1",
    "chai": "^4.0.1",
    "coveralls": "^2.11.11",
    "eslint": "^4.1.0",
    "eslint-config-lludol": "^0.1.6",
    "istanbul": "^0.4.4",
    "mocha": "^3.4.0",
    "mocha-lcov-reporter": "^1.2.0"
  }
}
